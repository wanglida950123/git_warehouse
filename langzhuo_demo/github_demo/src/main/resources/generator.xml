		<!--自动实现Serializable接口 -->
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>

		<!-- 去除自动生成的注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 数据库基本信息 -->

		<jdbcConnection driverClass="com.microsoft.sqlserver.jdbc.SQLServerDriver"
